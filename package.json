{
  "name": "site-react-front",
  "version": "0.0.1",
  "description": "DSNA/DTI playground for React components",
  "repository": {
    "type": "git",
    "url": "https://nexus.asap.dsna.fr/repository/npm-group/"
  },
  "publishConfig": {
    "registry": "https://nexus.asap.dsna.fr/repository/npm-releases/"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true
    }
  },
  "scripts": {
    "lint": "`npm bin`/eslint src/ --ext .js",
    "test": "NODE_ENV=production mocha -r esm --exit --timeout 20000 tests/**/*.spec.js",
    "dev": "DEBUG=CDM webpack-dev-server --config ./.config/webpack.js --display-error-details",
    "build": "webpack --mode production --config ./.config/webpack.js --display-error-details"
  },
  "author": "DSNA/DTI, Luc Thibault",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "express": "^4.17.1",
    "react": "^16.8.0",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.8.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.3.0",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "directories": {
    "conf": ".config",
    "test": "tests",
    "tools": "tools"
  }
}
